{
  "expo": {
    "name": "OpenVLab App",
    "slug": "openvlab-app",
    "version": "1.0.0",
    "scheme": "openvlab-app",
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-dev-client",
        {
          "launchMode": "most-recent"
        }
      ],
      "expo-font",
      "expo-web-browser"
    ],
    "experiments": {
      "reactCanary": true,
      "reactCompiler": true,
      "buildCacheProvider": {
        "plugin": "expo-build-disk-cache"
      },
      "tsconfigPaths": true,
      "typedRoutes": true
    },
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.OpenVLab.App",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.OpenVLab.App",
      "predictiveBackGestureEnabled": true
    },
    "newArchEnabled": true,
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "29eb71f0-e3bc-480b-8bc9-e739fb2db167"
      }
    }
  }
}
